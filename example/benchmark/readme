
# Example for testing RabbitMQ:

1) ./run_counter.sh        # run counter
2) ./rabbit/run.sh         # run rabbitmq
3) docker run -it --rm --network host -v `pwd`:/app py
  $ taskset 0xfffc python3 /app/rabbit/rb_worker.py &     # start worker
  $ taskset 0xfffc python3 /app/rabbit/rb_client.py &     # start client


# Inverted Json

1) ./run_counter.sh        # run counter
2) docker run -it --rm --network host --entrypoint sh lega911/ijson -c "taskset 0x1 /ijson --host 0.0.0.0"
3) docker run -it --rm --network host -v `pwd`:/app py
  $ taskset 0xfffc python3 /app/worker.py &     # start worker
  $ taskset 0xfffc python3 /app/client.py &     # start client
